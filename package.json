  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.12",
    "@types/text-encoding": "^0.0.36",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
